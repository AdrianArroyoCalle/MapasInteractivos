<?xml version='1.0' encoding='utf-8'?>
<widget id="mapas.interactivos.yayeyo" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Mapas Interactivos Yayeyo</name>
    <description>
       Mapas Interactivos de España, Europa, Asia, América, Oceanía y África. Practica y mejora en tus conocimientos de Geografía.
    </description>
    <author email="adrian.arroyocalle@gmail.com" href="http://adrianarroyocalle.github.io">
        Adrián Arroyo Calle
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
